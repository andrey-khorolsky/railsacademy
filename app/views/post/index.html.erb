<div>Posts</div>

<a href='/post/new'>write new post</a>

<%@post.each do |post|%>

    <div>
        <br>
        <div>From: <%=User.find(post.user_id).name%></div>
        <img src=<%=post.img%>>
        <div><%=post.text%></div>
        <div>Likes: <%=Like.findLikes post.id%></div>
    </div>

<%end%>
